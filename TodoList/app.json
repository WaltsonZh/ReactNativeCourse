{
  "expo": {
    "name": "TodoList",
    "slug": "TodoList",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/checklist.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/checklist.png",
      "resizeMode": "contain",
      "backgroundColor": "#383A57"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/checklist.png",
        "backgroundColor": "#383A57"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
